    </ItemGroup>

    <ItemGroup>
      <None Update="DataModels\GeopyDb.tt">
        <Generator>TextTemplatingFileGenerator</Generator>
        <LastGenOutput>GeopyDb.generated.cs</LastGenOutput>
      </None>
    </ItemGroup>

</Project>
